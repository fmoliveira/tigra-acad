@model List<Tigra.Areas.Admin.Models.TeamMember>
@if (Model.Count() == 0) {
    @:-
} else {
    <div class="list-group">
    @foreach (var m in Model) {
        <a class="list-group-item">
            <p class="list-group-item-heading"><strong>@Html.Raw(m.DisplayName)</strong></p>
            <p class="list-group-item-text">@Html.Raw(m.RoleName)</p>
        </a>
    }
    </div>
}